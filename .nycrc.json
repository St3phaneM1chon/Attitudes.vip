{
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "lcov",
    "html",
    "json-summary"
  ],
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "**/*.test.js",
    "**/*.spec.js",
    "**/node_modules/**",
    "**/coverage/**",
    "**/dist/**",
    "**/build/**",
    "**/scripts/**",
    "**/tests/**",
    "**/config/**",
    "**/__mocks__/**",
    "**/migrations/**",
    "**/seeds/**"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "cache": true,
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage"
}