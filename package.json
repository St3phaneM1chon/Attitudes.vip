{
  "name": "attitudes-vip",
  "version": "1.0.0",
  "description": "Application complÃ¨te de gestion de mariage",
  "main": "src/auth/auth-service.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "start:react": "react-scripts start",
    "build": "npm run build:css && npm run build:react",
    "build:css": "tailwindcss -i src/styles/input.css -o dist/output.css",
    "build:react": "react-scripts build",
    "build:js": "webpack --mode production",
    "test": "jest",
    "test:unit": "jest --testMatch=\"**/tests/unit/**/*.test.js\" --passWithNoTests",
    "test:watch": "jest --watch",
    "test:auth": "jest --testPathPattern=auth",
    "test:coverage": "jest --coverage",
    "test:fixed": "node scripts/run-tests-fixed.js",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:e2e:playwright": "playwright test",
    "test:e2e:playwright:ui": "playwright test --ui",
    "test:e2e:playwright:debug": "playwright test --debug",
    "test:integration": "node tests/run-integration-tests.js",
    "test:integration:hooks": "jest --testPathPattern=tests/hooks",
    "test:integration:services": "jest --testPathPattern=tests/websocket",
    "test:integration:report": "node tests/run-integration-tests.js --html-report",
    "test:integration:api": "node tests/integration/api/run-tests.js",
    "test:integration:api:coverage": "node tests/integration/api/run-tests.js --coverage",
    "test:integration:api:watch": "node tests/integration/api/run-tests.js --watch",
    "test:load": "node tests/websocket/load-test.js",
    "docker:build": "docker build -t attitudes-vip .",
    "docker:build:auth": "docker build -f Dockerfile.auth -t attitudes-vip-auth .",
    "docker:run": "docker run -p 8080:80 attitudes-vip",
    "docker:run:auth": "docker run -p 3000:3000 attitudes-vip-auth",
    "claude": "node scripts/claude-premium-assistant.js",
    "anchor:create": "node scripts/context-anchor.js create",
    "anchor:list": "node scripts/context-anchor.js list",
    "session:save": "node scripts/save-session-v2.js",
    "session:search": "node scripts/search-sessions.js",
    "teams:monitor": "node scripts/teams-session-monitor.js",
    "teams:dashboard": "./scripts/teams-power-commands.sh dashboard",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:restart": "docker-compose restart",
    "deploy": "npm run build && npm run docker:build",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "deploy:rollback": "./scripts/deploy.sh production rollback",
    "deploy:health": "./scripts/deploy.sh production health",
    "deploy:backup": "./scripts/deploy.sh production backup",
    "deploy:cleanup": "./scripts/deploy.sh production cleanup",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:staged": "lint-staged",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "security:scan": "trivy image attitudes-vip",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:init": "node scripts/init-db.js",
    "setup": "./scripts/setup-dev.sh",
    "audit": "node scripts/mega-deep-audit.js",
    "check": "./scripts/check-code-quality.sh",
    "db:backup": "pg_dump -h localhost -U postgres attitudes_vip > backup.sql",
    "db:restore": "psql -h localhost -U postgres attitudes_vip < backup.sql",
    "monitoring:start": "docker-compose --profile monitoring up -d",
    "monitoring:stop": "docker-compose --profile monitoring down",
    "monitoring:logs": "docker-compose --profile monitoring logs -f",
    "backup:start": "docker-compose --profile backup up -d",
    "backup:stop": "docker-compose --profile backup down",
    "performance:test": "k6 run scripts/performance-test.js",
    "performance:load": "artillery run scripts/load-test.yml",
    "docs:generate": "jsdoc src/ -d docs/api",
    "docs:serve": "http-server docs/api -p 8081",
    "prepare": "husky install"
  },
